'.source.advpl':
  'editor':
    'autoIndentOnPaste': false
    'softTabs': true
    'tabLength': 4
    'foldEndPattern': '^\\s*"""\\s*$'
    'commentStart': '// '
    'increaseIndentPattern': '\\b(?i:(function|if|for|while|elseif|else))\\b'
    'decreaseIndentPattern': '\\b(?i:(return|endif|next|enddo|elseif|else))\\b'
